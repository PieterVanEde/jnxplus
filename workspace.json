{
  "version": 2,
  "projects": {
    "checkstyle": {
      "root": "packages/checkstyle",
      "sourceRoot": "packages/checkstyle/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/checkstyle",
            "tsConfig": "packages/checkstyle/tsconfig.lib.json",
            "packageJson": "packages/checkstyle/package.json",
            "main": "packages/checkstyle/src/index.ts",
            "assets": [
              "packages/checkstyle/*.md",
              {
                "input": "./packages/checkstyle/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/checkstyle/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/checkstyle",
                "glob": "checkstyle.jar",
                "output": "."
              }
            ]
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "ktlint": {
      "root": "packages/ktlint",
      "sourceRoot": "packages/ktlint/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/ktlint",
            "tsConfig": "packages/ktlint/tsconfig.lib.json",
            "packageJson": "packages/ktlint/package.json",
            "main": "packages/ktlint/src/index.ts",
            "assets": [
              "packages/ktlint/*.md",
              {
                "input": "./packages/ktlint/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/ktlint/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/ktlint",
                "glob": "ktlint",
                "output": "."
              }
            ]
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "nx-boot-gradle": {
      "root": "packages/nx-boot-gradle",
      "sourceRoot": "packages/nx-boot-gradle/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/nx-boot-gradle",
            "tsConfig": "packages/nx-boot-gradle/tsconfig.lib.json",
            "packageJson": "packages/nx-boot-gradle/package.json",
            "main": "packages/nx-boot-gradle/src/index.ts",
            "assets": [
              "packages/nx-boot-gradle/*.md",
              {
                "input": "./packages/nx-boot-gradle/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-boot-gradle/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/nx-boot-gradle",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/nx-boot-gradle",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/nx-boot-gradle/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/nx-boot-gradle"],
          "options": {
            "jestConfig": "packages/nx-boot-gradle/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "nx-boot-gradle-e2e": {
      "root": "e2e\\nx-boot-gradle-e2e",
      "projectType": "application",
      "sourceRoot": "e2e\\nx-boot-gradle-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-boot-gradle:build",
            "jestConfig": "e2e/nx-boot-gradle-e2e/jest.config.js"
          }
        }
      }
    },
    "nx-boot-maven": {
      "root": "packages/nx-boot-maven",
